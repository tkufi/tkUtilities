{
  "name": "Ember Utilities",
  "tree": {
    "$className": "DataModel",

    "Workspace": {
      "$properties": {
        "FilteringEnabled": true,
        "StreamingEnabled": true,
        "ModelStreamingBehavior": "Improved"
      },
      "EmberUtilities": {
        "$path": "./src",
        "Packages": {
          "$path": "./Packages"
        }
      }
    }
  }
}
